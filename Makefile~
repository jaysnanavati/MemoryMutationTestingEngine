CC=gcc
CFLAGS=-g -Wall -Ihashset
VPATH = hashset

all: hashset.o
	$(CC) $(CFLAGS) -o `xml2-config --cflags --libs` Controller controller.c hashset/hashset.o valgrindEval.c  -Llibconfig  -lconfig
hashset.o:
	cd hashset && make

clean:
	rm -f *.o all
	rm -rf mutation_out
	cd hashset && make clean

.PHONY: clean
